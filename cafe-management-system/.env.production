NODE_ENV=production
PORT=5000

# Database Configuration - Update these for production
DB_HOST=your-production-db-host
DB_USER=your-production-db-user
DB_PASSWORD=your-production-db-password
DB_NAME=cafe_management
DB_PORT=3306

# JWT Configuration - Use strong secrets in production
JWT_SECRET=your-super-secure-jwt-secret-for-production-minimum-32-characters
JWT_EXPIRE=30d

# Bcrypt Configuration
BCRYPT_SALT_ROUNDS=12

# CORS Origins - Add your production frontend URL
FRONTEND_URL=https://your-frontend-domain.com